parameters:
	level: max
	scanFiles:
	  - wp-messaging.php
	bootstrapFiles:
	  - .tests/static-analysis/bootstrap.php
	scanDirectories:
		- wordpress
	paths:
		- inc
	ignoreErrors:
		- '#Parameter \#1 \$value of function strval expects bool\|float\|int\|resource\|string\|null, (.*) given.#'
		- '#Constant SD_WP_MESSAGING_PATH not found.#'
		- '#Parameter \#1 \$callback of function call_user_func expects callable\(\): mixed, (.*) given.#'